# Supabase Configuration
# Get these from your Supabase project dashboard: https://supabase.com/dashboard
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Privy Authentication
# Get this from your Privy dashboard: https://dashboard.privy.io
VITE_PRIVY_APP_ID=your_privy_app_id_here

# Paystack (Client)
# Public key is required for fiat payments. This is safe to expose to the client.
VITE_PAYSTACK_PUBLIC_KEY=your_paystack_public_key_here

# Production/Testing Environment Variables
# These are only needed for certain features and edge functions
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
# PAYSTACK_SECRET_KEY=your_paystack_secret_key_here
# UNLOCK_SERVICE_PRIVATE_KEY=your_unlock_service_private_key_here
# PRIVY_APP_SECRET=your_privy_app_secret_here
# PRIVY_VERIFICATION_KEY=your_privy_es256_public_key_here

# Note: Variables prefixed with VITE_ are exposed to the client
# Never put sensitive secrets in VITE_ variables

# Access Control (Unlock Protocol locks)
# These are used by the client to gate who can register schemas or insert them into the DB
VITE_ADMIN_LOCK_ADDRESS=
VITE_CREATOR_LOCK_ADDRESS=

# Primary Chain Selection
# The chain ID used for admin lock checks and other primary network operations.
# Defaults to Base Sepolia (84532) if unset.
VITE_PRIMARY_CHAIN_ID=

# Batch Attestation contract addresses
VITE_TEEREX_ADDRESS_BASE_MAINNET=
VITE_TEEREX_ADDRESS_BASE_SEPOLIA=

# Optional ERC-20 Token Addresses (Client)
# USDC addresses per network. Defaults are used if unset.
# Base Mainnet default: 0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913
VITE_USDC_ADDRESS_BASE_MAINNET=
# Base Sepolia default: 0x036CbD53842c5426634e7929541eC2318f3dCF7e
VITE_USDC_ADDRESS_BASE_SEPOLIA=
VITE_PRIMARY_CHAIN_ID=
VITE_ENABLE_FIAT=
VITE_DIVVI_CONSUMER_ADDRESS=0x374355b89D26325c4C4Cd96f99753b82fd64b2Bb